# Ozon Parser API - Environment Variables

# Google Sheets credentials (JSON string)
# Get from Google Cloud Console → Service Accounts → Create Key → JSON
GOOGLE_CREDENTIALS_JSON={"type":"service_account","project_id":"...","private_key":"..."}

# Or path to credentials file (alternative)
GOOGLE_CREDENTIALS_FILE=credentials/service-account.json

# Parser settings
PARSER_DELAY=2.5
PARSER_HEADLESS=true

# Server
PORT=8000
LOG_LEVEL=INFO
